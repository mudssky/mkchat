{
  "name": "mkchat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "format:check": "biome check --write .",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:migrate-dev": "dotenv -e .env.development prisma migrate dev",
    "prisma:migrate-prod": "dotenv -e .env.production prisma migrate deploy",
    "prisma:seed-dev": "dotenv -e .env.development prisma db seed",
    "prisma:studio-dev": "dotenv -e .env.development prisma studio --port 5555",
    "prisma:studio-prod": "dotenv -e .env.production prisma studio --port 5655",
    "typecheck": "tsc --noEmit",
    "typecheck:fast": "tsgo --noEmit",
    "qa": "pnpm typecheck:fast && pnpm lint && pnpm format:check",
    "setup:rules": "rule-loader  convert -t antigravity"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.1",
    "@ai-sdk/openai": "^3.0.1",
    "json-schema-to-zod": "^2.7.0",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "pino-pretty": "^13.1.3",
    "prisma": "^7.2.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.26.1+sha512.664074abc367d2c9324fdc18037097ce0a8f126034160f709928e9e9f95d98714347044e5c3164d65bd5da6c59c6be362b107546292a8eecb7999196e5ce58fa"
}
